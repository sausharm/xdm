  "$id": "https://ns.adobe.com/experience/campaign/orchestration/feedbackevent",
  "$schema": "http://json-schema.org/draft-06/schema#",
  "title": "User journey feedback event",
  "type": "object",
  "description": "",
  "definitions": {
    "feedbackevent": {
      "properties": {
        "xdm:timestamp": {
          "title": "Timestamp for feedback event",
          "type": "string",
          "format": "date-time",
          "description": "Time of generation of feedback event"
        },
        "xdm:messageId": {
          "title": "Message ID",
          "type": "string",
          "description": "A unique identifier across all services"
        },
        "xdm:externalID": {
          "title": "External ID",
          "type": "string",
          "description": "External ID, for example: delivery id"
        },
        "xdm:imsOrgID": {
          "title": "Ims Organizational ID",
          "type": "string",
          "description": "IMS org id of the tenant."
        },
        "xdm:instanceName": {
          "title": "Instance Name",
          "type": "string",
          "description": "Name of the instance"
        },
        "xdm:channelType": {
          "title": "Channel Type",
          "type": "string",
          "description": "The Channel type.",
          "meta:enum": {
            "email":
              "Email channel",
            "sms":
              "SMS channel",
            "push":
              "Push notification channel",
            "inAPP":
              "In app notification channel"
          }
        },
        "xdm:messagePriority": {
          "title": "Message Priority",
          "type": "string",
          "description": "Priority of a message"
        },
        "xdm:recipient": {
          "title": "recipient",
          "$ref": "https://ns.adobe.com/experience/campaign/orchestration/recipient",
          "description": "Details of the recipient."
        },
        "xdm:serviceName": {
          "title": "Service Name",
          "type": "string",
          "description": "The Name of the service.",
          "meta:enum": {
            "maas":
              "Messaging as a Service",
            "daas":
              "Delivery as a Service",
            "mcpns":
              "mcpns",
            "momentum":
              "momentum"
          }
        },
        "xdm:eventSourceName": {
          "title": "Event Source Name",
          "type": "string",
          "description": "Stage of the journey, for example: prep, mailmerge, delivery",
          "meta:enum": {
            "prep":
            "The prepration stage.",
            "mailmerge":
            "The mail merge stage",
            "delivery":
            "The delivery stage."
          }
        },
        "xdm:eventType": {
          "title": "Event Type",
          "type": "string",
          "description": "The Type of the Event.",
          "meta:enum": {
            "success":
              "Particular stage has been successfully passed.",
            "failure":
              "Particular stage has been failed."
          }
        },
        "xdm:eventCode": {
          "title": "event code",
          "type": "string",
          "description": "event code to understand the issue"
        },
        "xdm:eventMessage": {
          "title": "event Message",
          "type": "string",
          "description": "A more contextual message for developer to understand the issue"
        },
        "xdm:eventData": {
          "title": "event Data",
          "type": "object",
          "description": "custom event data"
        }
      }
    }
  },
  "required": [
    "xdm:timestamp",
    "xdm:messageId",
    "xdm:externalID",
    "xdm:imsOrgID",
    "xdm:channelType",
    "xdm:recipient",
    "xdm:serviceName",
    "xdm:eventType"
  ],
  "allOf": [
    {
      "$ref": "#/definitions/feedbackevent"
    }
  ],
  "meta:status": "stabilizing"
}